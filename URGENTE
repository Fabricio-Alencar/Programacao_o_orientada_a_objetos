        else:
            pass
    def adcionar_conta(self,conta):
        pass
        
    
class PessoaFisica(Cliente):
    def __init__(self, endereco, contas, cpf:str, nome:str, data_nascimento:str):
        super().__init__(endereco, contas)
        self.cpf = cpf
        self.nome = nome
        self.data_nascimento = data_nascimento
                         
                     
                         
